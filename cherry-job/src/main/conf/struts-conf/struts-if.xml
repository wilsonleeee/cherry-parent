<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
 <!--  ====================== 接口 Action ========================== -->
 <struts>
	<package name="if" namespace="/if" extends="cherry-default">
		<!--  ====================== 产品列表 start ========================== -->
		<!-- 产品导入 -->
		<action name="BINBEIFPRO01_init" class="com.cherry.ia.pro.action.BINBEIFPRO01_Action" method="binbeifpro01Exec">
    	</action>
		<!-- 产品下发 -->
		<action name="BINBEIFPRO02_init" class="com.cherry.ia.pro.action.BINBEIFPRO02_Action" method="binbeifpro02Exec">
    	</action>
		<!-- 柜台产品下发 -->
		<action name="BINBEIFPRO03_init" class="com.cherry.ia.pro.action.BINBEIFPRO03_Action" method="binbeifpro03Exec">
    	</action>
    	
		<!-- 产品下发(实时) -->
		<action name="BINBEIFPRO04_init" class="com.cherry.ia.pro.action.BINBEIFPRO04_Action" method="binbeifpro04Exec">
		</action>
		
		<!-- 特价产品下发 -->
		<action name="BINBEIFPRO05_init" class="com.cherry.ia.pro.action.BINBEIFPRO05_Action" method="binbeifpro05Exec">
		</action>
		<!-- ======================= 产品列表 end ========================== -->

		<!--  ====================== 柜台列表导入batch start ========================== -->
		<action name="BINBEIFCOU01_init" class="com.cherry.ia.cou.action.BINBEIFCOU01_Action" method="binbeifcou01Exec">
    	</action>
		<!-- ======================= 柜台列表导入batch end ========================== -->
		
		<!--  ====================== 柜台下发batch start ========================== -->
		<action name="BINBEIFCOU02_init" class="com.cherry.ia.cou.action.BINBEIFCOU02_Action" method="binbeifcou02Exec">
    	</action>
		<!-- ======================= 柜台列表导入batch end ========================== -->
		
		<!--  ====================== 营业员列表导入batch start ========================== -->
		<action name="BINBEIFEMP01_init" class="com.cherry.ia.emp.action.BINBEIFEMP01_Action" method="binbeifemp01Exec">
    	</action>
		<!-- ======================= 营业员列表导入batch end ========================== -->
		
		<!--  ====================== 员工列表导入batch start ========================== -->
		<action name="BINBEIFEMP02_init" class="com.cherry.ia.emp.action.BINBEIFEMP02_Action" method="binbeifemp02Exec">
    	</action>
    	<!-- ======================= 员工列表导入batch end ========================== -->
    	
    	<!--  ====================== 营业员列表导入（雅漾用）batch start ========================== -->
		<action name="BINBEIFEMP03_init" class="com.cherry.ia.emp.action.BINBEIFEMP03_Action" method="binbeifemp03Exec">
    	</action>
		<!-- ======================= 营业员列表导入（雅漾用）batch end ========================== -->
		<!--  ====================== CPA账号同步处理batch start ========================== -->
		<action name="BINBEIFEMP04_init" class="com.cherry.ia.emp.action.BINBEIFEMP04_Action" method="init">
			<result>/WEB-INF/jsp/ia/emp/BINBEIFEMP04.jsp</result>
    	</action>
    	<action name="BINBEIFEMP04_binbeifemp04Exec" class="com.cherry.ia.emp.action.BINBEIFEMP04_Action" method="binbeifemp04Exec">
    	</action>
		<!-- ======================= CPA账号同步处理batch end ========================== -->
		
		<!--  ====================== 刷新老后台U盘绑定柜台数据batch start ========================== -->
		<action name="BINBEIFEMP05_init" class="com.cherry.ia.emp.action.BINBEIFEMP05_Action" method="binbeifemp05Exec">
    	</action>
		<!-- ======================= 刷新老后台U盘绑定柜台数据batch end ========================== -->
		
		<!--  ====================== 部门列表导入batch start ========================== -->
		<action name="BINBEIFDEP01_init" class="com.cherry.ia.dep.action.BINBEIFDEP01_Action" method="binbeifdep01Exec">
    	</action>
		<!-- ======================= 部门列表导入batch end ========================== -->
		
		<!--  ====================== 员工管辖部门数据导入batch start ========================== -->
		<action name="BINBEIFDPL01_init" class="com.cherry.ia.dpl.action.BINBEIFDPL01_Action" method="binbeifdpl01Exec">
    	</action>
		<!-- ======================= 员工管辖部门数据导入batch end ========================== -->
		
		<!--  ====================== 区域信息下发batch start ========================== -->
		<action name="BINBEIFREG01_init" class="com.cherry.ia.reg.action.BINBEIFREG01_Action" method="binbeifreg01Exec">
    	</action>
		<!-- ======================= 区域信息下发batch end ========================== -->

		
	</package>
 </struts>