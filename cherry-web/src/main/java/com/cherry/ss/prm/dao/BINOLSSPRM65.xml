<sqlMap namespace="BINOLSSPRM65">
    <!--根据入库单ID删除单据内所有的明细-->
    <delete id="deletePrmInDepotDetail" parameterClass="java.util.HashMap">
        DELETE FROM
            Inventory.BIN_PrmInDepotDetail
        WHERE
            BIN_PrmInDepotID = #BIN_PrmInDepotID#
