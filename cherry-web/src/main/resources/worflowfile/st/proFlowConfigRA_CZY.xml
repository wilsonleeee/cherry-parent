<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE workflow PUBLIC "-//OpenSymphony Group//DTD OSWorkflow 2.8//EN" "http://www.opensymphony.com/osworkflow/workflow_2_8.dtd">
<!-- 配置产品退库申请流程（草之语）（提交退库申请单》一审》二审》终端退库） -->
<workflow>
    <initial-actions>
        <action id="1" name="Start Workflow">
            <pre-functions>
                <function type="class">
                    <arg name="class.name">com.opensymphony.workflow.util.Caller</arg>
                </function>
            </pre-functions>
            <results>
                <unconditional-result old-status="Finished" status="Underway" step="10">
                </unconditional-result>
            </results>
        </action>
    </initial-actions>
    <steps>
        <step id="10" name="提交退库申请单">
            <meta name="OS_StepShowFlag">1</meta>
            <meta name="OS_StepShowText">os.navigation.SubmitRABill</meta>
            <meta name="OS_StepShowOrder">1</meta>
            <meta name="OS_Rule">{"ThirdPartyFlag":"2","RuleType":"2","RuleOnFlowCode":"proFlowRA","RuleOnStep":[10],"RuleContext":[]}</meta>
            <actions>
                <action id="101" name="保存规则">
                    <meta name="OS_ButtonNameCode">os.navigation.Save</meta>
                    <meta name="OS_ButtonClass">icon-save</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="10">
                        </unconditional-result>
                    </results>
                </action>
                <action id="102" name="下一步">
                    <meta name="OS_ButtonNameCode">os.navigation.NextStep</meta>
                    <meta name="OS_ButtonClass">icon-mover</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="20">
                        </unconditional-result>
                    </results>
                </action>
            </actions>
        </step>
        <step id="20" name="一审">
            <meta name="OS_StepShowFlag">1</meta>
            <meta name="OS_StepShowText">os.navigation.Audit1</meta>
            <meta name="OS_StepShowOrder">2</meta>
            <meta name="OS_Rule">{"ThirdPartyFlag":"2","RuleType":"3","RuleOnFlowCode":"proFlowRA","RuleOnStep":[50],"RuleOnAction":[501,503,504],"RuleContext":[],"CanEditFlag":"true","PreferredFlag":"true"}</meta>
            <actions>
                <action id="201" name="上一步">
                    <meta name="OS_ButtonNameCode">os.navigation.PreStep</meta>
                    <meta name="OS_ButtonClass">icon-movel</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="10">
                        </unconditional-result>
                    </results>
                </action>
                <action id="202" name="保存规则">
                    <meta name="OS_ButtonNameCode">os.navigation.Save</meta>
                    <meta name="OS_ButtonClass">icon-save</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="20">
                        </unconditional-result>
                    </results>
                </action>
                <action id="203" name="下一步">
                    <meta name="OS_ButtonNameCode">os.navigation.NextStep</meta>
                    <meta name="OS_ButtonClass">icon-mover</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="30">
                        </unconditional-result>
                    </results>
                </action>
            </actions>
        </step>
        <step id="30" name="二审">
            <meta name="OS_StepShowFlag">1</meta>
            <meta name="OS_StepShowText">os.navigation.Audit2</meta>
            <meta name="OS_StepShowOrder">3</meta>
            <meta name="OS_Rule">{"ThirdPartyFlag":"2","RuleType":"3","RuleOnFlowCode":"proFlowRA","RuleOnStep":[55],"RuleOnAction":[551,553,554],"RuleContext":[],"CanEditFlag":"true","PreferredFlag":"true"}</meta>
            <actions>
                <action id="301" name="上一步">
                    <meta name="OS_ButtonNameCode">os.navigation.PreStep</meta>
                    <meta name="OS_ButtonClass">icon-movel</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="20">
                        </unconditional-result>
                    </results>
                </action>
                <action id="302" name="保存规则">
                    <meta name="OS_ButtonNameCode">os.navigation.Save</meta>
                    <meta name="OS_ButtonClass">icon-save</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="30">
                        </unconditional-result>
                    </results>
                </action>
                <action id="303" name="下一步">
                    <meta name="OS_ButtonNameCode">os.navigation.NextStep</meta>
                    <meta name="OS_ButtonClass">icon-mover</meta>
                    <results>
                        <unconditional-result old-status="Finished" status="Underway" step="40">
                        </unconditional-result>
                    </results>
                </action>
            </actions>
        </step>
        <step id="40" name="确认退库">
            <meta name="OS_StepShowFlag">1</meta>
            <meta name="OS_StepShowText">os.navigation.ConfirmRR</meta>
            <meta name="OS_StepShowOrder">4</meta>
            <meta name="OS_Rule">{"ThirdPartyFlag":"2","RuleType":"2","RuleOnFlowCode":"proFlowRA","RuleOnStep":[90],"RuleOnAction":[901,902],"RuleContext":[]}</meta>
            <actions>
                <action id="401" name="上一步">
                    <meta name="OS_ButtonNameCode">os.navigation.PreStep</meta>
                    <meta name="OS_ButtonClass">icon-movel</meta>
                    <results>
                        <unconditional-result old-status="Finished"  status="Underway" step="30">
                        </unconditional-result>
                    </results>
                </action>
                <action id="402" name="保存规则">
                    <meta name="OS_ButtonNameCode">os.navigation.Save</meta>
                    <meta name="OS_ButtonClass">icon-save</meta>
                    <results>
                        <unconditional-result old-status="Finished"  status="Underway" step="40">
                        </unconditional-result>
                    </results>
                </action>
                <action id="403" name="保存所有规则">
                    <meta name="OS_ButtonNameCode">os.navigation.SaveAll</meta>
                    <meta name="OS_ButtonClass">icon-save</meta>
                    <results>
                        <unconditional-result old-status="Finished"  status="Underway" step="40">
                        </unconditional-result>
                    </results>
                </action>
            </actions>
        </step>
        <step id="999" name="Flow over">
        </step>
    </steps>
</workflow>