<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- ============================= 会员活动 设置  Start============================= -->	
	<!--【积分兑礼】-->
	<bean id="actTemplateInitPX" class="com.cherry.cp.act.util.ActTemplateInitPX"/>
	<!--【积分兑现】-->
	<bean id="actTemplateInitPY" class="com.cherry.cp.act.util.ActTemplateInitPY"/>
	<!--【体验客领用】-->
	<bean id="actTemplateInitECR" class="com.cherry.cp.act.util.ActTemplateInitECR"/>
	<!--【老会员领用】-->
	<bean id="actTemplateInitOMR" class="com.cherry.cp.act.util.ActTemplateInitOMR"/>
	<!--【新会员二次购买】-->
	<bean id="actTemplateInitNMR" class="com.cherry.cp.act.util.ActTemplateInitNMR"/>
	<!--【生日礼】-->
	<bean id="actTemplateInitBIR" class="com.cherry.cp.act.util.ActTemplateInitBIR"/>
	<!--【入会礼】-->
	<bean id="actTemplateInitCG" class="com.cherry.cp.act.util.ActTemplateInitCG"/>
	<!--【COUPON优惠券】-->
	<bean id="actTemplateInitCP" class="com.cherry.cp.act.util.ActTemplateInitCP"/>
	<!--【试用装领用】-->
	<bean id="actTemplateInitSYZ" class="com.cherry.cp.act.util.ActTemplateInitSYZ"/>
	<!--【服务领用】-->
	<bean id="actTemplateInitSV" class="com.cherry.cp.act.util.ActTemplateInitSV"/>
	<!-- ============================= 会员活动 设置  End============================= -->
	
	<!-- ============================= 会员活动一览 Start============================= -->	
    <bean id="binOLCPACT01_Service" class="com.cherry.cp.act.service.BINOLCPACT01_Service"/>
    <bean id="binOLCPACT01_BL" class="com.cherry.cp.act.bl.BINOLCPACT01_BL"/>
    <!-- ============================= 会员活动一览 End============================= -->
    
    <!-- ============================= 会员活动详细 Start============================= -->
    <bean id="binOLCPACT02_Service" class="com.cherry.cp.act.service.BINOLCPACT02_Service"/>
    <bean id="binOLCPACT02_BL" class="com.cherry.cp.act.bl.BINOLCPACT02_BL"/>
    <!-- ============================= 会员活动详细 End============================= -->
    <!-- ============================= 会员活动批量预约 Start============================= -->
    <bean id="binOLCPACT05_Service" class="com.cherry.cp.act.service.BINOLCPACT05_Service"/>
    <bean id="binOLCPACT05_BL" class="com.cherry.cp.act.bl.BINOLCPACT05_BL"/>
    <!-- ============================= 会员活动批量预约 End============================= -->
   	
    <!-- ============================= 会员活动结果 Start============================= -->
    <bean id="binOLCPACT06_Service" class="com.cherry.cp.act.service.BINOLCPACT06_Service"/>
    <bean id="binOLCPACT06_BL" class="com.cherry.cp.act.bl.BINOLCPACT06_BL"/>
    <!-- ============================= 会员活动结果  End============================= -->
    
    <!-- =============================礼品领用报表Start================================ -->
    <bean id="binOLCPACT07_Service" class="com.cherry.cp.act.service.BINOLCPACT07_Service"/>
    <bean id="binOLCPACT07_IF" class="com.cherry.cp.act.bl.BINOLCPACT07_BL"/>
    <!-- =============================礼品领用报表End================================ -->
    
    <!-- =============================礼品领用详细一览Start================================ -->
    <bean id="binOLCPACT08_Service" class="com.cherry.cp.act.service.BINOLCPACT08_Service"/>
    <bean id="binOLCPACT08_IF" class="com.cherry.cp.act.bl.BINOLCPACT08_BL"/>
    <!-- =============================礼品领用详细一览End================================ -->
    
    <!-- ============================= 兑换结果 Start============================= -->
    <bean id="binOLCPACT10_Service" class="com.cherry.cp.act.service.BINOLCPACT10_Service"/>
    <bean id="binOLCPACT10_BL" class="com.cherry.cp.act.bl.BINOLCPACT10_BL"/>
    <!-- ============================= 兑换结果  End============================= -->
    
    <!-- =============================活动履历一览Start================================ -->
    <bean id="binOLCPACT11_Service" class="com.cherry.cp.act.service.BINOLCPACT11_Service"/>
    <bean id="binOLCPACT11_IF" class="com.cherry.cp.act.bl.BINOLCPACT11_BL"/>
    <!-- =============================活动履历一览End================================ -->
    
    <!-- =============================活动产品库存一览Start================================ -->
    <bean id="binOLCPACT12_Service" class="com.cherry.cp.act.service.BINOLCPACT12_Service"/>
    <bean id="binOLCPACT12_BL" class="com.cherry.cp.act.bl.BINOLCPACT12_BL"/>
    <!-- =============================活动产品库存一览End================================ -->
    
     <!-- =============================活动产品库存导入Start================================ -->
    <bean id="binOLCPACT13_Service" class="com.cherry.cp.act.service.BINOLCPACT13_Service"/>
    <bean id="binOLCPACT13_BL" class="com.cherry.cp.act.bl.BINOLCPACT13_BL"/>
    <!-- =============================活动产品库存导入End================================ -->
</beans>
